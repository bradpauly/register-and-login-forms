<%= form_for :registration_form, url: registrations_url do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.label :password %>:
  <%= f.text_field :password %><br />

  <%= f.label :password_confirmation %>:
  <%= f.text_field :password_confirmation %><br />

  <%= f.label :terms %>:
  <%= f.check_box :terms %><br />

  <%= f.submit %>
<% end %>